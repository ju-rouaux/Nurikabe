<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Text?>

<Scene xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <BorderPane fx:id="borderPane" prefHeight="480.0" prefWidth="640.0">
      <top>
         <Button fx:id="btn_retour" alignment="CENTER" mnemonicParsing="false" onAction="#retourClique" text="&lt;&lt;" BorderPane.alignment="TOP_LEFT" />
      </top>
      <center>
         <SplitPane dividerPositions="0.4" prefHeight="160.0" prefWidth="200.0" BorderPane.alignment="CENTER">
           <items>
               <BorderPane prefHeight="200.0" prefWidth="200.0">
                  <center>
                     <TableView fx:id="tableau_moi" BorderPane.alignment="CENTER">
                        <columns>
                           <TableColumn fx:id="score_moi" maxWidth="-1.0" minWidth="128.0" prefWidth="-1.0" resizable="true" text="score" />
                           <TableColumn fx:id="date_moi" maxWidth="-1.0" minWidth="128.0" prefWidth="-1.0" resizable="true" text="date" />
                        </columns>
                     </TableView>
                  </center>
                  <top>
                     <Text strokeType="OUTSIDE" strokeWidth="0.0" text="MOI" BorderPane.alignment="CENTER" />
                  </top>
               </BorderPane>
               <BorderPane prefHeight="200.0" prefWidth="200.0">
                  <center>
                     <TableView fx:id="tableau" BorderPane.alignment="CENTER">
                       <columns>
                           <TableColumn fx:id="nom" maxWidth="-1.0" minWidth="128.0" prefWidth="-1.0" resizable="true" text="nom" />
                         <TableColumn fx:id="score" maxWidth="-1.0" minWidth="128.0" prefWidth="-1.0" resizable="true" text="score" />
                           <TableColumn fx:id="date" maxWidth="-1.0" minWidth="128.0" prefWidth="-1.0" resizable="true" text="date" />
                       </columns>
                     </TableView>
                  </center>
                  <top>
                     <Text strokeType="OUTSIDE" strokeWidth="0.0" text="LES AUTRES" BorderPane.alignment="CENTER" />
                  </top>
               </BorderPane>
           </items>
         </SplitPane>
      </center>
   </BorderPane>
</Scene>
