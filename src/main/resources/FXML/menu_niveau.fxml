<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<Scene xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <root>
      <BorderPane fx:id="panneau">
         <top>
            <Button fx:id="btn_retour" mnemonicParsing="false" onAction="#retourClique" text="&lt;&lt;" BorderPane.alignment="CENTER" />
         </top>
         <bottom>
            <HBox fx:id="box" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="btn_page_prec" mnemonicParsing="false" onAction="#page_precedente" text="Précédent" />
                  <Label fx:id="num_page" text="0" />
                  <Button fx:id="btn_page_suiv" mnemonicParsing="false" onAction="#page_suivante" text="Suivant" />
               </children>
            </HBox>
         </bottom></BorderPane>
   </root>
   <stylesheets>
      <URL value="@../css/menu.css" />
      <URL value="@../css/global.css" />
   </stylesheets>
</Scene>
